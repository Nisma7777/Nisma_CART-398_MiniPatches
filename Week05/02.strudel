//02
setcpm(88)

$: note("0 2 4 7 5 4 2 0 2 4 5 7 9 7 5 4")
  .scale("C:major")
  .s("gm_acoustic_guitar_nylon")
  .legato(1.3)
  .room(0.5)
  .gain(1)
  .slow(2)

$: note("<0 4 5 3>*2").every(2 , x=> x.rev())
  .scale("C:major")
  .s("gm_bassoon")
  .slow(4)
  .room(0.8)
  .gain(0.8)
.delay(0.2).delayfeedback(0.3)
